10 REM *
20 REM * PROG 7.16 - Reversed Sounds
30 REM *
40 R = RND(-TIME)
50 FOR I = 0 TO 13
60 READ A : SOUND I,A
70 NEXT I
80 FOR I=1 TO  1000:NEXT
90 X=RND(I)*250
100 IF X<30 THEN 90
110 Y=RND(1)*10+1
120 SOUND 0,X : SOUND 1,Y
130 SOUND 2,X : SOUND 3,Y
140 SOUND 4,X+5 : SOUND 5,Y
150 SOUND 13,13
160 GOTO 80
170 DATA 62,2,60,2,60,2,0,56,16,16,16,120,30,13
9121 X=RND(I)*250
