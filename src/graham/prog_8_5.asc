10 REM *
20 REM * Relative LINE commands
30 REM *
40 COLOR 1,15,15
50 SCREEN 2
60 PSET( 170,60)
70 FOR I = 5 TO 55 STEP 5
80 LINE -STEP(I,0)
90 LINE -STEP(I,I)
100 LINE -STEP (0, I)
110 LINE -STEP(-I,I)
120 LINE -STEP(-I,0)
130 LINE -STEP(-I,-I)
140 LINE -STEP(0,-I)
150 LINE -STEP(I,-I)
160 PRESET STEP(-7,-5)
170 NEXT
180 FOR I = 1 TO 50 : NEXT I
190 CLS
200 PRESET (70,30)
210 FOR I = 1 TO 30
220 LINE STEP(-45,2)-STEP(50,0)
230 NEXT
240 FOR I = 1 TO 30
250 LINE STEP(-55,2)-STEP(50,0)
260 NEXT
270 GOTO 270
